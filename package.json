{
  "name": "moodify",
  "version": "1.0.0",
  "engines": {
    "node": "13.7.0"
  },
  "description": "Gives affirmations using text-to-speech tp help boost mood. PERN stack",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "build": "webpack",
