    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-redis-cache": "^1.1.3",
    "ibm-watson": "^5.5.0",
    "pg-promise": "^10.5.0"
  }
}
